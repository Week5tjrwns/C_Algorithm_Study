	#include <stdio.h>
#pragma warning(disable:4996) 
#include <string.h>
#include <time.h>


int main(void) {
	
	
	int N;
	int count = 0;
	scanf("%d", &N);
	static char buffer[20];
	static char array[10000][20];


	while (count != N) {
		static int i = 0, j = 666;
		snprintf(buffer, sizeof(buffer), "%d", j++);

		if ((strstr(buffer, "666")) != NULL) {
			strcpy(array[i++],buffer);
			count++;
		}


	}
	
	printf("%s", &array[N - 1]);

	return 0;
}
