#include<stdio.h>
#include<stdlib.h>
#include<string.h>
#pragma warning(disable:4996)

int power(int a, int b);
int main(void) {
	int a=0;
	int num = 0;
	int i = 0;
	int size = 0;
	static char arr[100] ;
	scanf("%s%d",arr,&a);
		
	size = strlen(arr);
	if (a > 10) {
		
		for (int i = size - 1; i >= 0; i--)
		{
			if ('A' <= *(arr + i)&& *(arr + i) <= 'Z')
			{
				num += ((int)*(arr + i) - 55) * power(a, size - 1 - i);
			}
			else if ('0' <= *(arr + i)&& *(arr + i) <= '9')
			{
				num += ((int)*(arr + i) - 48) * power(a, size - 1 - i);
			}
		}
	      
		printf("%d", num);
	}
	else {

		for (int i = size - 1; i >= 0; i--)
		{
			num += ((int)*(arr + i) - 48) * power(a, size - 1 - i);
		}
		printf("%d", num);
	}
	return 0;
}


int power(int a, int b)
{
	if (b == 0)return 1;
	return a* power(a,b - 1);
	
	 //a^b
}
