#include <stdio.h>
#pragma warning(disable:4996)

int func(int a1, int a0, int c, int n0);
int main(void) {
	int a1=0, a0=0, c=0, n0=0;

	(void)scanf("%d%d\n%d\n%d", &a1, &a0,&c,&n0);
	printf("%d",func(a1, a0, c, n0));

	return 0;
}

int func(int a1, int a0, int c, int n0)
{
	if (c - a1 >= 0) {
		if ((c - a1) * n0 >= a0)return 1;
		else return 0;
	}
	return 0;
	
	
}
