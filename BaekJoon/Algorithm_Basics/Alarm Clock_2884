#include<stdio.h>
#include<stdlib.h>

#pragma warning(disable:4996)
int main(void) {
	int num1=0,num2=0;
	scanf("%d%d", &num1, &num2);

	if (num2 < 45 && num1 != 0) {
		num2 = num2 + 15;
		num1--;
		printf("%d %d", num1, num2);
	}
	else if (num2 < 45 && num1 == 0) {
		num2 = num2 + 15;
		num1 = 23;
		printf("%d %d", num1, num2);
	}
	else printf("%d %d", num1, num2 - 45);
	return 0;
}
