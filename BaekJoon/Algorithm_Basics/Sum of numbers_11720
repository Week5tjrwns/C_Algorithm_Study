#include<stdio.h>
#include<string.h>
#include<stdlib.h>
#pragma warning(disable:4996)



int main(void) {
	int N = 0;
	int sum = 0;
	scanf("%d", &N);
	char* ptr = (char*)malloc(N * sizeof(char)); 
	scanf("%s", ptr); 
	for (int i = 0; i < N; i++) {

		sum += ptr[i] - 48;
	}
	printf("%d", sum);
	return 0;
}
