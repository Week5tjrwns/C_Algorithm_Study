#include<stdio.h>
#include<stdlib.h>
#pragma warning(disable:4996)

int main(void) {
	
	int arr[100];
	int num = 0;
	int num2 = 0;
	int count = 0;
	scanf("%d", &num);
	for (int i = 0; i < num; i++) {
		scanf("%d", arr + i);
	}
	scanf("%d", &num2);
	for (int i = 0; i < num; i++) {
		if (num2 == arr[i])++count;
	}
	printf("%d", count);
	return 0;
}
