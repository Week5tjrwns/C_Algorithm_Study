#include<stdio.h>
#include<stdlib.h>

#pragma warning(disable:4996)
int main(void) {
	int num1=0,num2=0,num3=0;
	scanf("%d%d%d",&num1,&num2,&num3);	
	int total = num1 * 60 + num2 + num3;
	int hour = total / 60;
	int minute = total % 60;
	
	if (hour >= 24)hour%= 24;

	if (hour == 24)hour = 0;
	printf("%d %d", hour, minute);
	return 0;
}
