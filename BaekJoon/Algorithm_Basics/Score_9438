#include<stdio.h>
#include<stdlib.h>
#pragma warning(disable:4996)

int main(void) {
	int A = 0;
	scanf("%d",&A);

	if (A >= 90&&A<=100)printf("A");
	else if (A <= 89 && A >= 80)printf("B");
	else if (A <= 79 && A >= 70)printf("C");
	else if (A <= 69 && A >= 60)printf("D");
	else printf("F");
	
	return 0;
}
