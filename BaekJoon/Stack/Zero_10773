#include <stdio.h>
#pragma warning(disable:4996)



int main(void) {

	int N = 0;
	scanf("%d", &N);
	static long arr[100001];
	long sum=0;
	int top = -1;
	long num = 0;
	for (int i = 0; i < N; i++) {
		scanf("%ld", &num);


		if (num == 0) {
			sum -= arr[top--];
		}
		else {
				sum += num;
				arr[++top] = num;

			}
			
		}


	


	printf("%d", sum);






	return 0;
}
