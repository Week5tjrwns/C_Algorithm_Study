#include <stdio.h>
#include <stdlib.h>
#include <string.h>

#pragma warning(disable:4996)
int main(void) {
	char arr[10] = {'\0'};
	int count = 0;
	scanf("%s",arr);

	for (int i = 0; i < 10; i++) {
		char temp = 0;
		for (int j =i ; j < 10; j++) {
			
			if (arr[i] < arr[j]) {
				temp=arr[i];
				arr[i] = arr[j];
				arr[j] = temp;
			}
		}
	}

	printf("%s", arr);
	

	return 0;
}
