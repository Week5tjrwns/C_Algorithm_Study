#include <stdio.h>
#pragma warning(disable:4996)
int main(void) {
    int T;
    scanf("%d", &T);
    int c0[41], c1[41];              
    c0[0] = 1; c1[0] = 0;            
    c0[1] = 0; c1[1] = 1;          
    for (int i = 2; i <= 40; ++i) {
        c0[i] = c0[i - 1] + c0[i - 2];   
        c1[i] = c1[i - 1] + c1[i - 2];
    }

    while (T--) {
        int n;
        scanf("%d", &n);
        printf("%d %d\n", c0[n], c1[n]);
    }

    return 0;
}
